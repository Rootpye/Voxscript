{
    "manifest_version": 3,
    "name": "Voxscript",
    "version": "1.0",
    "description": "Converts speech to text and supports multiple languages.",
    "action": {
      "default_title": "Speech to Text",
      "default_icon": {
        "16": "icon.png",
        "48": "icon.png",
        "64": "icon.png",
        "128": "icon.png"
      }
    },
    "icons": {
        "16": "icon.png",
        "48": "icon.png",
        "64": "icon.png",
        "128": "icon.png"
    },
    "permissions": [
      "activeTab",
      "storage",
      "microphone",
      "tabs"
    ],
    "background": {
      "service_worker": "background.js"
    }
  }
  
